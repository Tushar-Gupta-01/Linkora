<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/b"
    tools:context=".Sixteen">

    <TextView
        android:id="@+id/textView26"
        android:layout_width="370dp"
        android:layout_height="123dp"
        android:text="If You Have Any Kind Of Suggestions related to that particular course"
        android:textSize="30dp"
        android:textStyle="bold"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="78dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="290dp"
        android:layout_height="80dp"
        android:drawableRight="@drawable/ccp_ic_arrow_drop_down"
        android:text="Please Give Your Suggestion Here"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="217dp" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="279dp"
        android:layout_height="334dp"
        android:background="@color/white"
        android:ems="10"
        android:hint="Write Your Suggestions"
        android:inputType="textPersonName"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.746" />

    <EditText
        android:id="@+id/editTextTextPersonName9"
        android:layout_width="259dp"
        android:layout_height="72dp"
        android:ems="10"
        android:hint="College Id"
        android:textStyle="bold"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="324dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/round_pink_shape"
        android:backgroundTint="#F9A825"
        android:text="Feed"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.724"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="789dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#F9A825"
        android:text="Back"
        android:background="@drawable/round_pink_shape"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="810dp" />

</androidx.constraintlayout.widget.ConstraintLayout>